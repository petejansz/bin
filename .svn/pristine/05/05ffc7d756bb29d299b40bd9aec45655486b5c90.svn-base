param
(
    [string]$authcode,
    [string]$hostname,
    [int]$port,
    [switch]$help,
    [switch]$h 
)

Set-PSDebug -Off #Trace 2

. lib-register-ca-player.ps1

function showHelp()
{
    echo "USAGE: oauth-tokens -authcode <authcode> -hostname <hostname> -port <port>"
    exit 1
}

if ($h -or $help)    {showHelp}
if (-not($authcode)) {showHelp}
if (-not($hostname)) {showHelp}
if (-not($port))     {showHelp}

execOAuthTokens $hostname $port $authCode
